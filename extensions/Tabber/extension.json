{
	"name": "Tabber",
	"version": "2.4.5",
	"author": [
		"Eric Fortin",
		"Alexia E. Smith"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:Tabber",
	"descriptionmsg": "tabber-desc",
	"type": "parserhook",
	"license-name": "GPL-3.0-only",
	"requires": {
		"MediaWiki": ">= 1.31.0"
	},
	"MessagesDirs": {
		"Tabber": [
			"/i18n"
		]
	},
	"AutoloadClasses": {
		"Tabber\\TabberHooks": "TabberHooks.php"
	},
	"ResourceModules": {
		"ext.Tabber": {
			"styles": [
				"resources/ext.tabber.styles.less"
			],
			"scripts": [
				"resources/ext.tabber.js"
			],
			"targets": [
				"desktop",
				"mobile"
			],
			"dependencies": [
				"oojs-ui-core",
				"oojs-ui-widgets"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "Tabber"
	},
	"Hooks": {
		"ParserFirstCallInit": [
			"Tabber\\TabberHooks::onParserFirstCallInit"
		]
	},
	"manifest_version": 1
}
