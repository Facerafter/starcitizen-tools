{
	"name": "mediawiki/google-analytics",
	"type": "mediawiki-extension",
	"description": "Inserts Google Universal Analytics (and/or other web analytics) scripts into MediaWiki pages for tracking.",
	"keywords": [
		"MediaWiki",
		"GoogleAnalytics",
		"extension"
	],
	"homepage": "https://github.com/StarCitizenTools/mediawiki-extensions-GoogleAnalytics",
	"readme": "README.md",
	"license": "GPL-3.0",
	"require-dev": {
		"jakub-onderka/php-parallel-lint": "1.0.0",
		"mediawiki/mediawiki-codesniffer": "28.0.0",
		"jakub-onderka/php-console-highlighter": "0.4.0",
		"mediawiki/minus-x": "0.3.2",
		"mediawiki/mediawiki-phan-config": "0.9.0"
	},
	"scripts": {
		"test": [
			"parallel-lint . --exclude vendor --exclude node_modules",
			"phpcs --config-set ignore_warnings_on_exit 1",
			"phpcs -p -s",
			"minus-x check ."
		],
		"fix": [
			"minus-x fix ."
		]
	}
}
